SYNOPSIS

     use Complete::Man qw(complete_manpage);
    
     my $res = complete_manpage(word => 'gre');
     # -> ['grep', 'grep-changelog', 'greynetic', 'greytiff']
    
     # only from certain section
     $res = complete_manpage(word => 'gre', section => 1);
     # -> ['grep', 'grep-changelog', 'greytiff']

